      <script>
        (function(){
          if (Modernizr.svgclippaths) {
            var url = '../img/300.jpg';

            (new NB.CuttySark({
              debug: true,
              mediaqueries: {
                small: 'only screen and (min-width: 464px)',
                medium: 'only screen and (min-width: 752px)',
                large: 'only screen and (min-width: 1008px)',
                xlarge: 'only screen and (min-width: 1344px)',
                xxlarge: 'only screen and (min-width: 1920px)',
                retina: 'only screen and (-webkit-min-device-pixel-ratio: 2), ' +
                  'only screen and (min--moz-device-pixel-ratio: 2), ' +
                  'only screen and (-o-min-device-pixel-ratio: 2/1), ' +
                  'only screen and (min-device-pixel-ratio: 2), ' +
                  'only screen and (min-resolution: 192dpi), ' +
                  'only screen and (min-resolution: 2dppx)'
              }
            }))
              .init()
              .on('update', function(nextValues, oldValues) {
                var nextURL = '../img/300.jpg';

                if (_.includes(nextValues, 'retina')) {
                  nextURL = '../img/600.jpg';

                  if (_.includes(nextValues, 'large')) {
                    nextURL = '../img/2400.jpg';
                  }
                  else if (_.includes(nextValues, 'small')) {
                    nextURL = '../img/1200.jpg';
                  }
                }
                else {
                  if (_.includes(nextValues, 'large')) {
                    nextURL = '../img/1200.jpg';
                  }
                  else if (_.includes(nextValues, 'small')) {
                    nextURL = '../img/600.jpg';
                  }
                }

                if (nextURL !== url) {
                  url = nextURL;
                  jQuery('.frame svg image').attr('xlink:href', nextURL);
                }
              });
            }
        })();
      </script>
